{
  "name": "use-case-generator",
  "version": "0.0.1",
  "description": "The code generator parses a YAML file which contains a use-case description. Json-schema is used to specify the use-case parameters and response. The parsed use-case specification is used to generate the use-case interface as well as the parameter and response types.",
  "main": "build/index.js",
  "files": [
    "bin/"
  ],
  "scripts": {
    "start:dev": "npx nodemon --language C# --source ./examples/simple-type-ref.use-case.yml --destination ./.generated --ll Info",
    "build": "rimraf ./build && tsc"
  },
  "author": "Arjan Geertsema",
  "license": "Apache 2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/biz-dev-ops/use-case-generator"
  },
  "bin": {
    "gu": "build/index.js"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^10.1.0",
    "awilix": "^8.0.1",
    "js-yaml": "^4.1.0",
    "json-schema-merge-allof": "^0.8.1",
    "mustache": "^4.2.0",
    "pino": "^8.15.1",
    "pino-pretty": "^10.2.0",
    "quicktype-core": "^23.0.75",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/json-schema": "^7.0.12",
    "@types/json-schema-merge-allof": "^0.6.1",
    "@types/node": "^20.6.0",
    "@types/yargs": "^17.0.24",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
